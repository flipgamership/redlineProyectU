<div class="container">
  <div class="row">
    <h5 style="font-size: 20px; margin-bottom: 10px;" >Tabla de estadisticas de reportes diarios</h5>
    <hr>
    <table id="users" class="table table-light table-striped table-hover table-bordered text-center  " style="font-size: 14px;">
      <thead>
        <tr>
        <th>ID</th>
        <th>Nombre</th>
        <th>Cedula</th>
        <th>Telefono</th>
        <th>Plan</th>
        <th>Estado de llamada</th>
        <th>Tipificacion</th>
        <th>Observaciones</th>
        <th>administrar datos</th>
      </tr>
      </thead>

      <tbody>
        <% results.forEach((user) => {%>


        <tr>
          <td><%= user.id %> </td>
          <td><%= user.nombre %> </td>
          <td><%= user.cedula %></td>
          <td><%= user.telefono %></td>
          <td><%= user.plan %></td>
          <td><%= user.estadoLLamada %></td>
          <td><%= user.tipificacion %></td>
          <td><%= user.observaciones %></td>
          <td>
            <a href="/editTeletrabajoClient/<%= user.id %>" class="btn btn-warning" type="button"><i class='bx bx-pencil'></i></a>
            <a href="/telecomunicacionesValidarUsuario/<%= user.id %>" class="btn btn-success" type="button"><i class='bx bx-user-check'></i></a>
            <!-- <a href="/registerUsersDelate/" class="btn btn-danger" type="button"><i class='bx bxs-trash' ></i></a> -->
            <a onclick="alertDelate('<%= user.id %>')" class="btn btn-danger" type="button"><i class='bx bxs-trash'></i></a>
          </td>
        </tr>
        <% }) %>
      </tbody>
    </table>

  </div>
</div>
</body>

<script src="https://code.jquery.com/jquery-3.5.1.js"  ></script>
