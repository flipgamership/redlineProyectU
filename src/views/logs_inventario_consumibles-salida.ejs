<div class="container">
  <div class="row">
    <h5 style="font-size: 20px; margin-bottom: 10px;" >logs de consumibles salida del inventario</h5>
    <hr>
    <table id="users" class="table table-light table-striped table-hover table-bordered text-center  " style="font-size: 14px;">
      <thead>
        <tr>
        <th>ID del log</th>
        <th>ID consumibles</th>
        <th>Nombre del consumible</th>
        <th>cantidad anterior</th>
        <th>cantidad actual</th>
        <th>cantidad que salio del inventario</th>
        <th>precio total antes</th>
        <th>precio despues</th>
        <th>precio del gasto</th>
        <th>fecha</th>
      </tr>
      </thead>

      <tbody>
        <% results.forEach((user) => {%>


        <tr>
        <td ><%= user.id  %> </td>
        <td ><%= user.id_consumibles %></td>
        <td><%= user.objeto %></td>
        <td><%= user.cantidad_anterior %></td>
        <td><%= user.cantidad_actual %></td>
        <td><%= user.cantidad_retirada %></td>
        <td><%= user.precio_total_antes %></td>
        <td><%= user.precio_total_despues %></td>
        <td><%= user.	precio_gasto %></td>
        <td><%= user.	quien_retiro %></td>
        <td><%= user.fecha%></td>
        </tr>
        <% }) %>
      </tbody>
    </table>

  </div>
</div>
</body>

<script src="https://code.jquery.com/jquery-3.5.1.js"  ></script>
